{
  "name": "arquivei/php-kafka-consumer",
  "description": "A consumer of Kafka in PHP",
  "keywords": [
    "php",
    "kafka",
    "consumer"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "~7.4 || ~8 || ~9",
    "monolog/monolog": "~3",
    "illuminate/console": "~8.12 || ~9 || ~10",
    "ext-rdkafka": "~4 || ~5 || ~6"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.3"
  },
  "autoload": {
    "psr-4": {
      "Kafka\\Consumer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kafka\\Consumer\\Tests\\": "tests/"
    }
  },
  "archive": {
    "exclude": [
      "/.github",
      "/build",
      "/dev",
      "/tests",
      "Dockefile",
      "docker-compose.yaml",
      "Makefile",
      "phpunit.xml",
      "start.sh",
      "wait-for-it.sh"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kafka\\Consumer\\Laravel\\Providers\\PhpKafkaConsumerProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
